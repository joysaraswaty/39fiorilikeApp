<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m"
						 xmlns:f="sap.ui.layout.form">
	<f:SimpleForm>
		<f:content>
			<Label text="Supplier Id"></Label>
			<Text text="{ToSupplier/BP_ID}"></Text>
			<Label text="Supplier Name"></Label>
			<Text text="{ToSupplier/COMPANY_NAME}"></Text>
			<Label text="Address"></Label>
			<Text text="{ToSupplier/STREET},{ToSupplier/BUILDING}"></Text>
			<Label text="Location"></Label>
			<Text text="{ToSupplier/CITY},{ToSupplier/COUNTRY}"></Text>
		</f:content>
	</f:SimpleForm>
</core:FragmentDefinition>